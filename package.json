{
  "name": "guildbot",
  "version": "0.1.0",
  "description": "Minimal Discord bot that sends JSON prompts to an LLM via HTTP and returns JSON responses (TypeScript)",
  "main": "dist/index.js",
  "scripts": {
    "start": "NODE_ENV=production tsx src/index.ts",
    "format": "prettier --write .",
    "postinstall": "npm link @hexafield/agent-workflow",
    "dev": "NODE_ENV=development tsx watch src/index.ts",
    "lint": "tsc --noEmit && eslint . --ext .ts",
    "eslint:fix": "eslint . --ext .ts --fix",
    "test": "NODE_ENV=test vitest run",
    "ui:server": "tsx watch ui/server/index.ts",
    "ui:web": "vite --config ui/web/vite.config.ts",
    "ui:dev": "concurrently -k -n server,web -c green,cyan \"npm:ui:server\" \"npm:ui:web\""
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.19.0",
    "@lancedb/lancedb": "^0.23.0",
    "@mermaid-js/mermaid-cli": "^11.12.0",
    "@resvg/resvg-js": "^2.6.2",
    "@snazzah/davey": "^0.1.7",
    "app-root-path": "^3.1.0",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "csv-parse": "^5.4.0",
    "d3": "^7.9.0",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "msgpackr": "^1.11.5",
    "multer": "^2.0.2",
    "ollama": "^0.6.0",
    "opusscript": "^0.0.8",
    "prism-media": "^1.3.5",
    "solid-js": "^1.9.3",
    "ws": "^8.18.3",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "cross-fetch": "^3.1.8",
    "eslint": "^8.47.0",
    "postcss": "^8.4.49",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "tailwindcss": "^3.4.15",
    "tsx": "^4.21.0",
    "typescript": "^5.4.2",
    "vite": "^5.4.10",
    "vite-plugin-solid": "^2.10.2",
    "vitest": "^1.2.1"
  },
  "engines": {
    "node": ">=18"
  },
  "bin": {
    "guildbot": "./dist/cli.js"
  }
}
